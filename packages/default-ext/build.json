{
  "entry": {
    "engine-ext": "./src/index.tsx"
  },
  "library": "VitisLowCodeEngineExt",
  "libraryTarget": "umd",
  "externals": {
    "react": "var window.React",
    "react-dom": "var window.ReactDOM",
    "vitis-lowcode-engine": "var window.VitisLowCodeEngine",
    "vitis-lowcode-default-ext": "var window.VitisLowCodeEngineExt"
  },
  "polyfill": false,
  "outputDir": "dist",
  "vendor": false,
  "ignoreHtmlTemplate": true,
  "sourceMap": true,
  "plugins": [
    "build-plugin-react-app",
    "./build.plugin.js"
  ]
}
